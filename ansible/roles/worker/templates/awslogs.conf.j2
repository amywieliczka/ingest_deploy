[akara-error.log]
datetime_format = {{ log_datetime_format }}
file = {{ dir_venv }/akara-run-dir/logs/error.log
buffer_duration = 5000
log_stream_name = {{ name_env }}-{instance_id}-{ip_address}
initial_position = start_of_file
log_group_name = {{ dir_venv }/akara

[worker-log]
datetime_format = {{ log_datetime_format }}
file = {{ dir_venv }/rqworker/worker.log
buffer_duration = 5000
initial_position = start_of_file
log_stream_name = {{ name_env }}-{instance_id}-{ip_address}
log_group_name = {{ dir_venv }/worker
